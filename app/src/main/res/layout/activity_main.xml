    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.allenxuan.xuanyihuang.isrlproject.MainActivity">
    <com.allenxuan.xuanyihuang.inheritedswiperefreshlayout.swiperefreshlayout.InheritedSwipeRefreshLayout
        android:layout_width="match_parent"
